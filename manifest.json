{
  "manifest_version": 2,

  "name": "AwesomeTab",
  "description": "This extension shows a nice and customizable tab by opening a new tab",
  "version": "1.1",

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
	"16": "icon16.png",
	"48": "icon48.png",
	"128": "icon128.png"
  },
  "permissions": [
	"storage"
  ]
}
